{
  "expo": {
    "description": "An app to learn React and demonstrate to Virta that I'd be a great fit as a developer there",
    "primaryColor": "#28acfc",
    "name": "Joe Kalb POC App for Virta",
    "slug": "JoeKalbVirtaApp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/virta-favicon.png",
    "privacy": "public",
    "splash": {
      "image": "./assets/virta-favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kalbifornia.JoeKalbVirtaApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/virta-favicon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/virta-favicon.png"
    }
  }
}
